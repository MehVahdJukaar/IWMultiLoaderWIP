{
  "multipart": [
    {
      "when": {
        "smoldering": "false"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_post"
      }
    },
    {
      "when": {
        "north": "true",
        "smoldering": "false"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true",
        "smoldering": "false"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true",
        "smoldering": "false"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true",
        "smoldering": "false"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "smoldering": "true"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_post_lit"
      }
    },
    {
      "when": {
        "north": "true",
        "smoldering": "true"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side_lit",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true",
        "smoldering": "true"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side_lit",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true",
        "smoldering": "true"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side_lit",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true",
        "smoldering": "true"
      },
      "apply": {
        "model": "immersive_weathering:block/charred_fence_side_lit",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}